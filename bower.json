{
  "name": "upcycle",
  "version": "0.0.0",
  "homepage": "http://adsk-ui.github.io/upcycle/",
  "authors": [
    "admangum <adam.mangum@autodesk.com>"
  ],
  "description": "A home for our efforts to transition one-offs and semi-vetted code into a library of high quality, well tested, reusable components",
  "main": [
    "build/upcycle.js",
    "build/themes/base/base.css"
  ],
  "keywords": [
    "js",
    "components"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "src"
  ],
  "dependencies": {
    "jquery": "1.8.3",
    "jquery-ui": "~1.10.4",
    "handlebars": "~1.3.0",
    "underscore": "~1.6.0",
    "tinyscrollbar": "https://github.com/wieringen/tinyscrollbar.git#~2.1.6",
    "jquery-i18n-properties": "https://github.com/gejiawen/jquery.i18n.properties.git#1.1.1"
  },
  "devDependencies": {
    "mocha": "~1.18.2",
    "chai": "~1.9.1",
    "bootstrap": "2.3.2"
  },
  "overrides": {
    "jquery-ui": {
      "main": [
        "ui/jquery.ui.core.js",
        "ui/jquery.ui.widget.js"
      ]
    },
    "tinyscrollbar": {
      "main": "lib/jquery.tinyscrollbar.js"
    },
    "mocha": {
      "main": [
        "mocha.css",
        "mocha.js"
      ]
    }
  }
}
